var youstars = angular.module('youstars', [], function ($routeProvider, $locationProvider) {
  $locationProvider.html5Mode(true);
  $routeProvider.when('', {
    templateUrl: '<%= asset_path("index.html") %>',
    controller: 'indexController'
  }).when('/test', {
    templateUrl: '<%= asset_path("home.html") %>',
  }).when('/:currentChannel', {
    templateUrl: '<%= asset_path("index.html") %>',
    controller: 'indexController'
  })
})